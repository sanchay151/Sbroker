{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/user/(.*)", "dest": "/index.js" },
    { "src": "/profile/(.*)", "dest": "/index.js" },
    { "src": "/stock/(.*)", "dest": "/index.js" },
    { "src": "/watchlist/(.*)", "dest": "/index.js" },
    { "src": "/", "dest": "/index.js" }
  ]
}
